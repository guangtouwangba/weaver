    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "@tailwindcss/postcss": "^4",
    "@types/rbush": "^4.0.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^11.18.2",
    "konva": "^10.0.12",
    "lucide-react": "^0.562.0",
