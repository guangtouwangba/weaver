{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "node -e \"try { require.resolve('husky') } catch (e) { process.exit(0) }\" && cd ../.. && husky app/frontend/.husky || true"
