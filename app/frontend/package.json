  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "eslint-config-prettier": "^10.1.8",
    "fast-check": "^3.22.0",
    "husky": "^9.1.7",
